@import "./styles/iconfont.wxss";
page,
view,
text,
swiper,
swiper-item,
image,
navigator {
    padding: 0;
    margin: 0;
    box-sizing: border-box;
}


/*
    主题颜色
    1. 在less中存在变量的知识
    2. 原生的css和wxss也是支持变量的
*/

page {
    /* 定义主题颜色 */
    --themeColor: #eb4450;
    /* 定义统一的字体大小*/
    font-size: 28rpx;
}